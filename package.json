{
  "name": "@roadmunk/events",
  "version": "2.0.0",
  "description": "The JS Protobufs generated by protoc.",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:ci": "jest --ci --reporters=default --reporters=jest-junit",
    "test:watch": "jest --watchAll",
    "build": "mkdir -p ./dist && docker run --rm -v $(pwd):$(pwd) -w $(pwd) znly/protoc --js_out=import_style=commonjs,binary:dist -I . protos/*.proto",
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix ./src",
    "docs": "jsdoc -c .jsdoc.js",
    "docs:watch": "nodemon --exec 'npx jsdoc -c .jsdoc.js' --watch ./src ./tutorials"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/@roadmunk"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Roadmunk/events.git"
  },
  "author": "Jared Bydeley <jared@roadmunk.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Roadmunk/events/issues"
  },
  "homepage": "https://github.com/Roadmunk/events#readme",
  "dependencies": {
    "google-protobuf": "^3.9.1",
    "sqs-consumer": "^5.4.0"
  },
  "devDependencies": {
    "@roadmunk/eslint-config-roadmunk": "^3.17.1",
    "better-docs": "^2.0.1",
    "eslint": "^7.0.0",
    "jest": "^26.0.1",
    "jsdoc": "^3.6.4",
    "nodemon": "^2.0.4"
  }
}
